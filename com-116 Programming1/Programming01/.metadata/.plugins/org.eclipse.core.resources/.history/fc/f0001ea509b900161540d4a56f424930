import java.util.*;

public class Problem1155 {

	public static void main(String[] args) {
		Locale.setDefault(Locale.US); 
		double a = 1.00;
		double b = 1.00;
		double s = 0;
		while (b<=100) {
			a/=b;
			++b;
			s+=a;
			System.out.printf("%.2f\n", b);
		}
		System.out.printf("%.2f\n", s);
	}
}
