import java.util.*;

public class Problem1151 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		int a = input.nextInt();
		int b = 0;
		int c = a;
		int fac = 1;
		while (b<a) {
			++b;
			fac *= c;
			c-=b;
		}
		System.out.println(fac);
	}
}
