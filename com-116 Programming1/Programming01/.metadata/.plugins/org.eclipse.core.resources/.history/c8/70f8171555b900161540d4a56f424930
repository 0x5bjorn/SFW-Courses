import java.util.*;

public class Problem1159 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		boolean run = true;
		int a = 0;
		int b = 0;
		int sum = 0;
		while (run) {
			int x = input.nextInt();
			a += x;
			if (x == 0) {
				run = false;
			} else {
				while (b < 5) {
					if (a % 2 == 0) {
						sum += a;
						++b;
					}
					++a;
				}
			}
		}
		run = false;
		System.out.println(sum);
	}
}
